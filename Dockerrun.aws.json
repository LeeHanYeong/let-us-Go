{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "azelf/letusgo:nginx",
            "memory": 300,
            "mountPoints": [
                {
                    "containerPath": "/srv/project/.static",
                    "sourceVolume": "Static"
                }
            ],
            "name": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "links": [
                "web"
            ]
        },
        {
            "command": [
                "bash",
                "-c",
                "python3 manage.py collectstatic --noinput && gunicorn config.wsgi.production:application --bind 0.0.0.0:8000 --chdir /srv/project/app --access-logfile /var/log/gunicorn-access.log --error-logfile /var/log/gunicorn-error.log"
            ],
            "essential": true,
            "image": "347809506342.dkr.ecr.ap-northeast-2.amazonaws.com/letusgo:django",
            "memory": 400,
            "mountPoints": [
                {
                    "containerPath": "/srv/project/.static",
                    "sourceVolume": "Static"
                }
            ],
            "name": "web"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "static"
            },
            "name": "Static"
        }
    ],
    "AWSEBDockerrunVersion": 2
}